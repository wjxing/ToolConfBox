export GOROOT=$HOME/root/external/go
export GOPATH=$HOME/root/external/go/extern:$HOME/Workspace/Golang
export __GOBIN__=${GOPATH//://bin:}/bin
export PATH=$GOROOT/bin:$__GOBIN__:$HOME/root/usr/bin:$PATH

source $HOME/root/etc/bash_completion.d/*

if [ ! -n $SSH_TTY ]; then
    export DISPLAY='192.168.1.104:0'
fi
